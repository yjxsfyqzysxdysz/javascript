{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [false],
        "semicolon": [false],
        "interface-name": [true, "never-prefix"],
        "object-literal-sort-keys": [false],
        "no-console": [false],
        "variable-name": [false],
        "no-angle-bracket-type-assertion": [false],
        "array-type": [false],
        "no-empty": [false],
        "no-shadowed-variable": [false],
        "ban-types": [false],
        "no-unused-expression": [false],
        "forin": [false],
        "max-classes-per-file": false,
        "no-namespace": [false],
        "no-reference": [false]
    },
    "rulesDirectory": []
}